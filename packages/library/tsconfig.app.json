{
  "extends": "@vue/tsconfig/tsconfig.lib.json",
  "include": ["env.d.ts", "./src/**/*", "./src/**/*.vue", "./src/utils/**/*"],
  "exclude": ["src/**/__tests__/*"],
  "compilerOptions": {
    "composite": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"]
    },
    "skipLibCheck": true
  }
}
